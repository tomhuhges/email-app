{
  "parserOptions": {
  "ecmaVersion": 6,
  "ecmaFeatures": {
    "jsx": true
  },
  "sourceType": "module"
  },
  "env": {
    "browser": true,
    "es6": true,
    "node": true,
    "mocha": true
  },
  "plugins": [
    "react"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "airbnb"
  ],
  "rules": {
    "import/no-extraneous-dependencies": [0, { "devDependencies": ["test/**"] }]
  }
}
